            }
		});
		////////////////////////////////////////////////////////

		// ДИНАМИЧЕСКАЯ ПОДГРУЗКА НУЖНЫХ БЛОКОВ ////////////
		var lastClass;
		$('.dynamic').click( function(event) {
			event.preventDefault();
			addStyle = $( this ).attr('data-style');
			name = $( this ).children('li').text();
			_url = '/dynamic/add' + $(this).attr('href') + '';
			console.log(_url);
			$.ajax({
				url: _url,
				method: 'GET',
				statusCode: {
					200: function (req,res,err) {
						console.log(req);
						$('.title-best').removeClass('default-float-color');
						$('.title-best').removeClass(lastClass);
						$('.news-all-best').html('');
						$('.title-best').html('');
						for (i in req) {
	                    	beta = parseInt(i) + 1 ;
	                    	content = $('<a href="' + req[i]._id + '"><div class="news-all-best-each"><div class="news-all-best-each-title">' + req[i].title_in + '</div></div></a>').hide();
	                    	$('.news-all-best').append(content.show('slow'));
	                	}				
						$('.title-best').text(name).addClass(addStyle);
						lastClass = addStyle;
					}
				} 
			})
		});
		///////////////////////////////////////////////

		// ПОЛОСА СЛЕВА - НАВЕРХ
		$("#go-top").on('click', function(){

      		$('html, body').animate({
       			scrollTop: 0
   			}, 50);
       	}); 
		//////////////////////////////////////

		if ((navigator.userAgent.match(/iPhone/i)) 
		  	||  (navigator.userAgent.match(/iPod/i)) 
			||  (navigator.userAgent.match(/Android/i))) 
			{
				$('img').each(function() {
					var height = Math.floor($(this).attr('height') * 1.18787) + 1;
					var width = Math.floor($(this).attr('width') * 1.18787) + 1;
					$(this).attr('width', width).attr('height', height).css('margin', '0px 0px');
				})
				$('iframe').each(function() {
					var height = Math.floor($(this).attr('height') * 1.18787) + 1;
					var width = Math.floor($(this).attr('width') * 1.18787) + 1;
					$(this).parent().css('width', width).css('margin', '0px 0px');
					$(this).attr('width', width).attr('height', height).css('margin', '0px 0px');
				})
			}
	</script>
	
