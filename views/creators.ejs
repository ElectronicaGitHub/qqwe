			success: function(req,res,err) {
                console.log(req);
                for (i in req) {
                	beta = parseInt(i) + 1 ;
                	$('.news-all-best').append('<a href="' + req[i]._id + '"><div class="news-all-best-each"><div class="news-all-best-each-title">' + req[i].title_in + '</div></div></a>');
